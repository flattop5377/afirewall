#!/usr/bin/make -f

#%:
#	dh $@ --with python3

BASE=afirewall/usr/share/afirewall

default :
	echo "Doing nothing"

${BASE} :
	mkdir -p ${BASE}


${BASE}/afirewall : ${BASE} ../afirewall/afirewall
	cp ../afirewall/afirewall ${BASE}/afirewall

build build-arch build-indep : ${BASE}/afirewall

binary binary-arch binary-indep : ${BASE}/afirewall

.PHONY : clean

clean :
	rm -rf afirewall

patch :

