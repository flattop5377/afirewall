#!/usr/bin/make -f

default :
	echo "Supports Debian build targets see: https://www.debian.org/doc/debian-policy/ch-source.html#main-building-script-debian-rules"

build build-arch build-indep :

binary binary-arch binary-indep : 
	dh_installdirs
	dh_install
	dh_link
	dh_fixperms

.PHONY : clean

clean :
	dh_clean

patch : build

